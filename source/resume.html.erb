<% @hide_footer = true %>

<div class="section section--padded">
  <div class="section__content resume">
    <address class="resume__header">
      <h2 class="resume__author">
        <%= current_page.data.name %>
      </h2>

      <div class="resume__contact-info">
        <p>
          <% current_page.data.address.each do |address_line| %>
            <%= address_line %><br />
          <% end %>
        </p>
      </div>

      <div class="resume__contact-info">
        <%= link_to(
          abridge_url(current_page.data.technical_portfolio_url),
          current_page.data.technical_portfolio_url,
        ) %>

        <%= link_to(
          current_page.data.email,
          "mailto:#{current_page.data.email}",
          class: "u-link--incognito",
        ) %>

        <%= link_to(
          current_page.data.phone,
          "tel:+1#{current_page.data.phone.gsub(/\D/, '')}",
          class: "u-link--incognito",
        ) %>
      </div>
    </address>

    <% if current_page.data.bio %>
      <h2 class="resume__heading">
        Summary
      </h2>

      <section class="resume__section">
        <p>
          <%= current_page.data.bio %>
        </p>
      </section>
    <% end %>

    <h2 class="resume__heading">
      Professional Experience
    </h2>

    <section class="resume__section">
      <% current_page.data.work.each do |experience| %>
        <%= partial "resume/experience", locals: { experience: experience } %>
      <% end %>
    </section>

    <h2 class="resume__heading">
      Memberships
    </h2>

    <section class="resume__section">
      <% current_page.data.memberships.each do |experience| %>
        <%= partial "resume/experience", locals: { experience: experience } %>
      <% end %>
    </section>

    <% if current_page.data.appearances_html %>
      <h2 class="resume__heading">
        Speaking
      </h2>

      <section class="resume__section">
        <ul>
          <% current_page.data.appearances_html.each do |appearance| %>
            <li>
              <%= appearance %>
            </li>
          <% end %>
        </ul>
      </section>
    <% end %>

    <% if current_page.data.technical_skills %>
      <h2 class="resume__heading">
        Technical Skills
      </h2>

      <section class="resume__section">
        <p>
          <%= current_page.data.technical_skills.join(", ") %>
        </p>
      </section>
    <% end %>

    <h2 class="resume__heading">
      Education
    </h2>

    <section class="resume__section">
      <% current_page.data.education.each do |experience| %>
        <%= partial "resume/experience", locals: { experience: experience } %>
      <% end %>
    </section>
  </div>
</div>
